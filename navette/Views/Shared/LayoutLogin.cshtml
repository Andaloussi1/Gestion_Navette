
@model navette.Models.USER_APP
@{
    Layout = null;
}
@using navette.Models;
<!DOCTYPE html>
<html lang="en-US" dir="ltr">



<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">




    <!-- ===============================================-->
    <!-- Document Title-->
    <!-- ===============================================-->
    <title>Ma Navette , Officiel Site Of Ma Navette&amp; ANDALOUSSI & BELRHITI </title>




    <!-- ===============================================-->
    <!-- Favicons-->
    <!-- ===============================================-->
    <link rel="apple-touch-icon" sizes="180x180" href="~/Content/LastTemp/assets/img/favicons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/Content/LastTemp/assets/img/favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/LastTemp/assets/img/favicons/favicon-16x16.png">
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/LastTemp/assets/img/favicons/favicon.ico">
    <link rel="manifest" href="~/Content/LastTemp/assets/img/favicons/manifest.json">
    <meta name="msapplication-TileImage" content="~/Content/LastTemp/assets/img/favicons/mstile-150x150.png">
    <meta name="theme-color" content="#ffffff">




    <!-- ===============================================-->
    <!-- Stylesheets-->
    <!-- ===============================================-->
    <link href="~/Content/LastTemp/assets/css/theme.css" rel="stylesheet" />



</head>
<body>
    <!-- ===============================================-->
    <!-- Main Content-->
    <!-- ===============================================-->
    <main class="main" id="top">
        <nav class="navbar navbar-expand-lg navbar-light fixed-top py-3 d-block" data-navbar-on-scroll="data-navbar-on-scroll">
            <div class="container">
                <a class="navbar-brand" href="index.html"><img class="d-inline-block" src="~/Content/LastTemp/assets/img/gallery/logo.png" width="50" alt="logo" /><span class="fw-bold text-primary ms-2">Navett'ON</span></a>
                <button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
                <div class="collapse navbar-collapse border-top border-lg-0 mt-4 mt-lg-0" id="navbarSupportedContent">
                    <ul class="navbar-nav mx-auto pt-2 pt-lg-0 font-base">
                        <li class="nav-item px-2"><a class="nav-link fw-medium active" aria-current="page" href="#destinations"><span class="nav-link-text">Home </span></a></li>
                        <li class="nav-item px-2"><a class="nav-link" href="#flights"> <span class="nav-link-text">About us</span></a></li>
                        <li class="nav-item px-2"><a class="nav-link" href="#hotels"> <span class="nav-link-text">Contact us</span></a></li>
                    </ul>
                    
                        <li class="btn text-800 order-1 order-lg-0 me-2"><a href="@Url.Action("Login", "GestionNavette")" class="nav-link">Sign in</a></li>
                        <li class="btn btn-voyage-outline order-0"><a href="@Url.Action("Register", "GestionNavette")" class="nav-link">Register</a></li>
                    



                </div>
            </div>
        </nav>



        @RenderBody()



        <!--/.bg-holder-->
        <!-- ============================================-->
        <!-- <section> begin ============================-->
        <!-- end of .container-->



    </main>
    <!-- ===============================================-->
    <!-- End of Main Content-->
    <!-- ===============================================-->
    <!-- ===============================================-->
    <!-- JavaScripts-->
    <!-- ===============================================-->
    <script src="~/Content/LastTemp/vendors/bootstrap/bootstrap.min.js"></script>
    <script src="~/Content/LastTemp/vendors/popperjs/popper.min.js"></script>
    <script src="~/Content/LastTemp/vendors/is/is.min.js"></script>
    <script src="https://polyfill.io/v3/polyfill.min.js?features=window.scroll"></script>
<script src="~/Content/LastTemp/vendors/fontawesome/all.min.js"></script>
    <script src="~/Content/LastTemp/assets/js/theme.js"></script>



    <link href="https://fonts.googleapis.com/css2?family=Titillium+Web:wght@300;400;600;700&display=swap" rel="stylesheet">
</body>
</html>